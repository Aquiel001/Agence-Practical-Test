
@{
        ViewBag.Title = "Con_desempenho_aba_cliente";
}
<script>
    function llamarVistaParcial(tbox) {
        var counter = [];
        for (var i = 0; i < tbox.options.length; i++) {
            counter.push(tbox.options[i].text);
        }

        var laURLDeLaVista = '@Url.Action("_Con_desempenho_aba_client", "Home")';
    $.ajax({
        cache: false,
        async: true,
        type: "POST",
        url: laURLDeLaVista,
        data: { count: counter },
        success: function (response) {
            $('#resultado').html('');
            $('#resultado').html(response);
        }
    });
}
</script>

