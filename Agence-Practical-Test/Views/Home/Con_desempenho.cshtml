<script>
    function llamarVistaParcial(tbox, monthfrom, yearfrom, monthto, yearto) {
        var names = [];
        for (var i = 0; i < tbox.options.length; i++) {
            names.push(tbox.options[i].text);
        }
        var laURLDeLaVista = '@Url.Action("_Con_desempenho", "Home")';
    $.ajax({
        cache: false,
        async: true,
        type: "POST",
        url: laURLDeLaVista,
        data: { coUsuario: names, initDate: monthfrom.value+'/'+ yearfrom.value, finalDate: monthto.value +'/'+ yearto.value },
        success: function (response) {
            $('#resultado').html('');
            $('#resultado').html(response);
        }
    });
    }

      function llamarVistaParcialGraf(tbox, monthfrom, yearfrom, monthto, yearto) {
        var names = [];
