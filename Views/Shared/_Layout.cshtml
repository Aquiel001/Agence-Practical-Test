                        </div>
                    </li>
                    <li style="padding-left:10px">
                        <div>
                            <img id="im4" src="/img/menu/fichario.gif" border="0">
                        </div>
                        <div>
                            <a href="?Length=4">Financiero</a>
                        </div>
                    </li>
                    <li style="padding-left:10px">
                        <div>
                            <img id="im5" src="/img/icone_inf_pessoal.gif" border="0">
                        </div>
                        <div>
                            <a href="?Length=4">Usuario</a>
                        </div>
                    </li>
                    <li style="padding-left:10px">
                        <div>
                            <img id="im6" src="/img/cancel.gif" border="0">
                        </div>
                        <div>
                            <a href="/Home/Index">Salir</a>
                        </div>
                    </li>
                </ul>

            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
    </div>

    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")*@
    
    @RenderSection("scripts", required: false)
</body>
</html>
