        <div class="container">

            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

                @Html.ActionLink("CAOL", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>

            <div class="navbar-collapse collapse">

                <ul class="nav navbar-nav">
                    <li style="padding-left:10px">
                        <div style="align-content:center">
                            <img id="im0" src="/img/menu/agence.gif" border="0">
                        </div>
                        <div>
                            <a length="59" href="?Length=4">
                                Agence
                            </a>
                        </div>
                    </li>
                    <li style="padding-left:10px">
                        <div>
                            <img id="im1" src="/img/menu/task_icon.gif" border="0">
                        </div>
                        <div>
                            <a href="?Length=4">Proyectos</a>
                        </div>
                    </li>
                    <li style="padding-left:10px">
                        <div>
                            <img id="im2" src="/img/menu/administrativo.gif" border="0">
                        </div>
                        <div>
                            <a href="?Length=4">Administrativo</a>
                        </div>
                    </li>
                    <li style="padding-left:10px">
                        <div>
                            <img id="im3" src="/img/menu/comercial.gif" border="0">
